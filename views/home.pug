doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a.button(href="/update-cobj") Add to this table
        
        table
            thead
                tr
                    th Name
                    th Age
                    th Type
            tbody
                each pet in customObjects
                    tr
                        td= pet.properties.pet_name
                        td= pet.properties.pet_age
                        td= pet.properties.pet_type